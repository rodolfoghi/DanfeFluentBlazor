@using DanfeFluentBlazor.Extensions
@using NFe.Classes

<FluentGrid>
    <FluentGridItem xs="12">
        <FluentCard>
            <IdentificacaoDaNotaFiscalEletronica IdentificacaoDaNFe="@nfeProc.NFe.infNFe.ide" />
        </FluentCard>
    </FluentGridItem>
    <FluentGridItem xs="12">
        <FluentCard>
            <Emitente nfeProc="@nfeProc" />
        </FluentCard>
    </FluentGridItem>
    <FluentGridItem xs="12">
        <FluentCard>
            <Destinatario Dest="@nfeProc.NFe.infNFe.dest" />
        </FluentCard>
    </FluentGridItem>
</FluentGrid>


@code {
    [Parameter]
    public nfeProc nfeProc { get; set; } = new();
}
